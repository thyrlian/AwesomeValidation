        <activity
            android:name="com.basgeekball.awesomevalidation.demo.DemoActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
